{
  "presets": [
    "env",
    "flow",
    "react"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-function-bind",
    "transform-class-properties",
    [
      "module-resolver", {
        "root": ["./src"]
      }
    ]
  ],
  "env": {
    "server": {
      "plugins": [
        [
          "css-modules-transform", {
            "generateScopedName": "[hash:8]",
            "extensions": [".css"]
          }
        ]
      ]
    }
  }
}
